        <!-- Customize your theme here. -->
        <item name="colorPrimary">#433a52</item>
        <item name="colorPrimaryDark">#292333</item>
        <item name="colorAccent">#3d1e5e</item>
        <item name="android:fontFamily" tools:targetApi="jelly_bean">monospace</item>
